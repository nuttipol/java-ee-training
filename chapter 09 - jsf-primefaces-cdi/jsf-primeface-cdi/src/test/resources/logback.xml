<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender"> 
		<encoder>
			 <pattern>%d{HH:mm:ss.SSS} %-5level [%X{sessionId}][%logger] %m%n</pattern>
		</encoder>
	</appender>
	 
	<root level="TRACE">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>